{
  "$schema": "https://raw.githubusercontent.com/sourcegraph/sourcegraph/master/shared/src/schema/extension.schema.json",
  "name": "@sourcegraph/lsif-tsc",
  "description": "A tool to create an LSIF dump for TypeScript projects.",
  "sideEffects": false,
  "publisher": "sourcegraph",
  "version": "0.6.8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:sourcegraph/lsif-node"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "./dist/src/main.js",
  "bin": {
    "lsif-tsc": "./dist/src/main.js"
  },
  "files": [
    "dist",
    "src/*.js",
    "LICENSE"
  ],
  "scripts": {
    "prettier": "prettier --write --list-different '**/*.{ts,js?(on),md,yml}'",
    "prettier-check": "yarn run prettier --write=false",
    "tslint": "tslint -p tsconfig.json --format stylish",
    "eslint": "eslint --cache '**/*.ts?(x)'",
    "build": "node ./node_modules/typescript/bin/tsc -b .",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "lru-cache": "^5.1.1",
    "lsif-protocol": "0.4.3",
    "minimist": "^1.2.0",
    "npm": "^6.14.2",
    "typescript-lsif": "https://github.com/dbaeumer/TypeScript/releases/download/4.1.3-lsif.1/typescript-4.1.3-lsif.1.tgz",
    "uuid": "^7.0.2",
    "vscode-uri": "^2.1.1"
  },
  "devDependencies": {
    "@sourcegraph/eslint-config": "0.24.0",
    "@sourcegraph/prettierrc": "3.0.3",
    "@sourcegraph/tsconfig": "4.0.1",
    "@sourcegraph/tslint-config": "13.4.0",
    "@types/lru-cache": "5.1.0",
    "@types/minimist": "1.2.1",
    "@types/node": "13.13.51",
    "@types/npm": "2.0.31",
    "@types/uuid": "7.0.4",
    "eslint": "7.23.0",
    "prettier": "2.2.1",
    "ts-node": "^8.6.2",
    "tslint": "6.1.3",
    "typescript": "^3.8.3"
  }
}
