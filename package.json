{
  "$schema": "https://raw.githubusercontent.com/sourcegraph/sourcegraph/master/shared/src/schema/extension.schema.json",
  "name": "@sourcegraph/lsif-tsc",
  "description": "A tool to create an LSIF dump for TypeScript projects.",
  "sideEffects": false,
  "publisher": "sourcegraph",
  "version": "0.5.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:sourcegraph/lsif-node"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "./dist/src/main.js",
  "bin": {
    "lsif-tsc": "./dist/src/main.js"
  },
  "files": [
    "dist/",
    "LICENSE"
  ],
  "scripts": {
    "prettier": "prettier --write --list-different '**/*.{ts,js?(on),md,yml}'",
    "prettier-check": "yarn run prettier --write=false",
    "tslint": "tslint -p tsconfig.json --format stylish",
    "eslint": "eslint --cache '**/*.ts?(x)'",
    "build": "node ./node_modules/typescript/bin/tsc -b .",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "lru-cache": "^5.1.1",
    "lsif-protocol": "0.4.3",
    "minimist": "^1.2.0",
    "npm": "^6.10.3",
    "typescript-lsif": "https://github.com/dbaeumer/TypeScript/releases/download/release%2Flisf%2F3.5.2/typescript-3.5.2-lsif.tgz",
    "uuid": "^3.3.2",
    "vscode-uri": "^2.0.2"
  },
  "devDependencies": {
    "@sourcegraph/eslint-config": "0.9.2",
    "@sourcegraph/prettierrc": "3.0.2",
    "@sourcegraph/tsconfig": "4.0.0",
    "@sourcegraph/tslint-config": "13.4.0",
    "@types/lru-cache": "5.1.0",
    "@types/minimist": "1.2.0",
    "@types/node": "13.7.4",
    "@types/npm": "2.0.31",
    "@types/uuid": "3.4.8",
    "eslint": "6.8.0",
    "prettier": "1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "5.20.1",
    "typescript": "^3.7.5"
  }
}
